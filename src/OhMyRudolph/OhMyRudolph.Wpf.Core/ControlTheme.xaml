<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">
    <!--  Global  -->
    <FontFamily x:Key="GlobalFont">Malgun Gothic</FontFamily>
    <SolidColorBrush x:Key="RudolphNoseColor">Red</SolidColorBrush>

    <!--  MainWindow.xaml  -->
    <Color x:Key="Background_MainWindow">#FF131313</Color>
    <SolidColorBrush x:Key="Brush_Background_MainWindow" Color="{StaticResource Background_MainWindow}" />

    <!--  MainPageView.xaml  -->
    <sys:Double x:Key="Width_PhotoGrayMainPageButton">400</sys:Double>
    <sys:Double x:Key="Height_PhotoGrayMainPageButton">50</sys:Double>

    <!--  PhotoView.xaml  -->
    <Thickness x:Key="Margin_PageTitle">20</Thickness>
    <HorizontalAlignment x:Key="HorizontalAlignment_PageTitle">Center</HorizontalAlignment>
    <VerticalAlignment x:Key="VerticalAlignment_PageTitle">Center</VerticalAlignment>
    <HorizontalAlignment x:Key="HorizontalAlignment_BottomButtonSet">Left</HorizontalAlignment>
    <Thickness x:Key="Margin_BottomButtonSet">20,0,20,20</Thickness>
    <Orientation x:Key="Orientation_BottomButtonSet">Horizontal</Orientation>
    <sys:Double x:Key="Width_BottomButtonSet">150</sys:Double>
    <sys:Double x:Key="Height_BottomButtonSet">40</sys:Double>
    <Thickness x:Key="Interval_BottomButtonSet">0,0,10,0</Thickness>
    <sys:Double x:Key="Width_EffectButtonSetItemContent">30</sys:Double>
    <sys:Double x:Key="Height_EffectButtonSetItemContent">30</sys:Double>
    <Thickness x:Key="Margin_EffectButtonSetItemContent">5</Thickness>
    <sys:Double x:Key="Width_EffectButtonSetItem">130</sys:Double>
    <sys:Double x:Key="Height_EffectButtonSetItem">130</sys:Double>
    <Thickness x:Key="Margin_EffectButtonSetItem">5</Thickness>
    <SolidColorBrush x:Key="Background_EffectButtonSetItem">#FF353535</SolidColorBrush>
    <SolidColorBrush x:Key="BorderBrush_EffectButtonSetItem">#FF606060</SolidColorBrush>
    <Thickness x:Key="BorderThickness_EffectButtonSetItem">2</Thickness>

    <!--  PhotoGrayBackButton.xaml  -->
    <Color x:Key="BorderBrush_PhotoGrayBackButton">#FF404040</Color>
    <SolidColorBrush x:Key="Brush_BorderBrush_PhotoGrayBackButton" Color="{StaticResource BorderBrush_PhotoGrayBackButton}" />
    <Color x:Key="Background_PhotoGrayBackButton">#FF202020</Color>
    <SolidColorBrush x:Key="Brush_Background_PhotoGrayBackButton" Color="{StaticResource Background_PhotoGrayBackButton}" />
    <Thickness x:Key="Thickness_PhotoGrayBackButton">1.5</Thickness>
    <sys:Double x:Key="FontSize_PhotoGrayBackButton">18</sys:Double>
    <FontWeight x:Key="FontWeight_PhotoGrayBackButton">DemiBold</FontWeight>
    <Color x:Key="Foreground_PhotoGrayBackButton">#FFFFFFFF</Color>
    <SolidColorBrush x:Key="Brush_Foreground_PhotoGrayBackButton" Color="{StaticResource Foreground_PhotoGrayBackButton}" />
    <CornerRadius x:Key="CornerRadius_PhotoGrayBackButton">25</CornerRadius>
    <sys:Double x:Key="BackSymbol_PhotoGrayBackButton">8</sys:Double>

    <!--  PhotoGrayEffectSelectionButton.xaml  -->
    <sys:Double x:Key="WidthHeight_PhotoGrayEffectSelectionButton">150</sys:Double>
    <CornerRadius x:Key="CornerRadius_PhotoGrayEffectSelectionButton">5</CornerRadius>

    <!--  PhotoGrayVideoViewPhotoShotButton.xaml  -->
    <sys:Double x:Key="Width_PhotoGrayVideoViewPhotoShotButton">110</sys:Double>
    <sys:Double x:Key="Height_PhotoGrayVideoViewPhotoShotButton">140</sys:Double>
    <CornerRadius x:Key="CornerRadius_PhotoGrayVideoViewPhotoShotButton">5</CornerRadius>

    <!--  PhotoGrayMainPageButton.xaml  -->
    <Color x:Key="BorderBrush_PhotoGrayMainPageButton">#FFFFFFFF</Color>
    <SolidColorBrush x:Key="Brush_BorderBrush_PhotoGrayMainPageButton" Color="{StaticResource BorderBrush_PhotoGrayMainPageButton}" />
    <Color x:Key="Background_PhotoGrayMainPageButton">#FF131313</Color>
    <SolidColorBrush x:Key="Brush_Background_PhotoGrayMainPageButton" Color="{StaticResource Background_PhotoGrayMainPageButton}" />
    <Thickness x:Key="Thickness_PhotoGrayMainPageButton">1.5</Thickness>
    <sys:Double x:Key="FontSize_PhotoGrayMainPageButton">18</sys:Double>
    <FontWeight x:Key="FontWeight_PhotoGrayMainPageButton">DemiBold</FontWeight>
    <Color x:Key="Foreground_PhotoGrayMainPageButton">#FFFFFFFF</Color>
    <SolidColorBrush x:Key="Brush_Foreground_PhotoGrayMainPageButton" Color="{StaticResource Foreground_PhotoGrayMainPageButton}" />
    <CornerRadius x:Key="CornerRadius_PhotoGrayMainPageButton">25</CornerRadius>

    <!--  PhotoGrayModeSelectionCardButton.xaml  -->
    <Color x:Key="BorderBrush_PhotoGrayModeSelectionCardButton">#FF353535</Color>
    <SolidColorBrush x:Key="Brush_BorderBrush_PhotoGrayModeSelectionCardButton" Color="{StaticResource BorderBrush_PhotoGrayModeSelectionCardButton}" />
    <Thickness x:Key="Thickness_PhotoGrayModeSelectionCardButton">2</Thickness>
    <CornerRadius x:Key="CornerRadius_PhotoGrayModeSelectionCardButton">20</CornerRadius>
    <sys:Double x:Key="FontSize_PhotoGrayModeSelectionCardButton">23</sys:Double>
    <FontWeight x:Key="FontWeight_PhotoGrayModeSelectionCardButton">Medium</FontWeight>
    <VerticalAlignment x:Key="VerticalAlignment_PhotoGrayModeSelectionCardButton">Center</VerticalAlignment>
    <HorizontalAlignment x:Key="HorizontalAlignment_PhotoGrayModeSelectionCardButton">Center</HorizontalAlignment>

    <!--  ReadyScreen.xaml  -->
    <Color x:Key="Foreground_ReadyScreen">#FFFFFFFF</Color>
    <SolidColorBrush x:Key="Brush_Foreground_ReadyScreen" Color="{StaticResource Foreground_ReadyScreen}" />
    <FontWeight x:Key="FontWeight_ReadyScreen">DemiBold</FontWeight>
    <VerticalAlignment x:Key="VerticalAlignment_ReadyScreen">Center</VerticalAlignment>
    <HorizontalAlignment x:Key="HorizontalAlignment_ReadyScreen">Center</HorizontalAlignment>
    <Color x:Key="PathStroke_ReadyScreen">#FFB62F2F</Color>
    <SolidColorBrush x:Key="Brush_PathStroke_ReadyScreen" Color="{StaticResource PathStroke_ReadyScreen}" />
    <Thickness x:Key="PathThickness_ReadyScreen">2</Thickness>
    <sys:Double x:Key="FontSize_ReadyScreen_Ready">120</sys:Double>
    <sys:Double x:Key="FontSize_ReadyScreen_CountNotice">25</sys:Double>
    <sys:Double x:Key="FontSize_ReadyScreen_TextBlock">30</sys:Double>
    <HorizontalAlignment x:Key="HorizontalAlignment_ReadyScreen_PhotoGraySpeechBubble">Left</HorizontalAlignment>
    <Thickness x:Key="Margin_ReadyScreen_PhotoGraySpeechBubble">180,0,0,0</Thickness>

    <!--  VideoView.xaml  -->
    <sys:Double x:Key="Width_DetectingNoseView">850</sys:Double>
    <sys:Double x:Key="Height_DetectingNoseView">650</sys:Double>
    <sys:Double x:Key="Width_ButtonSet">600</sys:Double>
    <sys:Double x:Key="Height_ButtonSet">180</sys:Double>
    <VerticalAlignment x:Key="VerticalAlignment_ButtonSet">Bottom</VerticalAlignment>
    <HorizontalAlignment x:Key="HorizontalAlignment_ButtonSet">Center</HorizontalAlignment>
    <Thickness x:Key="Margin_ButtonSet">0,0,0,20</Thickness>
    <SolidColorBrush x:Key="Background_ButtonItem">#FF353535</SolidColorBrush>
    <SolidColorBrush x:Key="BorderBursh_ButtonItem">#FF606060</SolidColorBrush>
    <Thickness x:Key="BorderThickness_ButtonItem">2</Thickness>
    <sys:Double x:Key="WidthHeight_ButtonItemContent">30</sys:Double>
    <HorizontalAlignment x:Key="HorizontalAlignment_PhotoShotButton">Right</HorizontalAlignment>

    <!--  FinalPageView.xaml  -->
    <Color x:Key="Foreground_FinalPageView">#FFFFFFFF</Color>
    <SolidColorBrush x:Key="Brush_Foreground_FinalPageView" Color="{StaticResource Foreground_FinalPageView}" />
    <FontWeight x:Key="FontWeight_FinalPageView">DemiBold</FontWeight>
    <VerticalAlignment x:Key="VerticalAlignment_FinalPageView">Center</VerticalAlignment>
    <HorizontalAlignment x:Key="HorizontalAlignment_FinalPageView">Center</HorizontalAlignment>
    <sys:Double x:Key="FontSize_FinalPageView_Thankyou">80</sys:Double>
    <sys:Double x:Key="FontSize_FinalPageView_Seeyou">25</sys:Double>
</ResourceDictionary>