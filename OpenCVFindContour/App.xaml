<Application x:Class="OpenCVFindContour.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:local="clr-namespace:OpenCVFindContour"
             mc:Ignorable="d"
             xmlns:vm="clr-namespace:OpenCVFindContour.ViewModel"
             xmlns:cn="clr-namespace:OpenCVFindContour.Converter"
             StartupUri="View/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <vm:ViewModelLocator x:Key="Locator"/>
            <cn:BitmapToBitmapSourceConverter x:Key="BitmapToBitmapSourceConverter"/>
            <cn:MatToWriteableBitmapConverter x:Key="MatToWriteableBitmapConverter"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
